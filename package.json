{
  "name": "react-esquio",
  "version": "1.0.1",
  "description": "Esquio FeatureFlags for React",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "jest",
    "prebuild": "rimraf dist/",
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "version": "git add -A src"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yello001/react-esquio.git"
  },
  "keywords": [
    "esquio",
    "react"
  ],
  "author": "Yello",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Yello001/react-esquio/issues"
  },
  "homepage": "https://github.com/Yello001/react-esquio#readme",
  "devDependencies": {
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^5.1.0",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.2",
    "jest": "^26.6.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17"
  },
  "dependencies": {
    "esquio-js-connector": "^1.0.2"
  }
}
